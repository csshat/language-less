{
  "name": "csshat-language-less-lesshat",
  "title": "Less (Less Hat)",
  "version": "1.1.0",
  "description": "Less language",
  "main": "dust.js",
  "scope": "source.css.less",
  "engines": {
    "csshat": "~2.0.0",
    "avocode": "*"
  },
  "homepage": "https://github.com/csshat/language-less-lesshat",
  "repository": {
    "type": "git",
    "url": "https://github.com/csshat/language-less-lesshat"
  },
  "bugs": {
    "url": "https://github.com/csshat/language-less-lesshat/issues"
  },
  "author": "Petr Brzek <petrbrzek@abdoc.net> (https://github.com/petrbrzek)",
  "contributors": [
    "Marek Hrabe <marekhrabe@abdoc.net> (https://github.com/marekhrabe)",
    "Tomas Hanacek <tomashanacek@abdoc.net> (https://github.com/tomashanacek)"
  ],
  "license": "MIT",
  "dependencies": {
    "octopus-helpers": "^0.2.9"
  },
  "settings": {
    "inheritFontStyles": {
      "type": "boolean",
      "default": true
    },
    "selector": {
      "type": "boolean",
      "default": false
    },
    "selectorTextStyle": {
      "type": "hidden",
      "options": [
        "Dash case",
        "Snake case",
        "Camel case"
      ],
      "default": "Dash case"
    },
    "selectorType": {
      "type": "select",
      "options": [
        "id",
        "class",
        "element"
      ],
      "default": "class"
    },
    "colorType": {
      "type": "select",
      "options": [
        "hex",
        "rgb",
        "hsl"
      ],
      "default": "hex"
    },
    "safeMode": {
      "type": "boolean",
      "default": false
    },
    "showTextSnippet": {
      "type": "boolean",
      "default": true
    }
  },
  "bundledDependencies": [
    "octopus-helpers"
  ]
}
